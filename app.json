{
      "name": "Hacker World String Hack Bot",
      "description": "Telegram Account Hacks Bot",
      "logo": "https://telegra.ph/file/6c2829ce8b491552e07c0.jpg",
      "keywords": [
         "telegram",
         "anime",
         "group",
         "manager",
         "innexi"
      ],   
   "repository": "https://github.com/hackerworldyt/StringHackBot.git",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-python"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    }
  ],  
   "env": {
      "BOT_TOKEN": {
         "description": "Your bot token. Get one from @BotFather duh",
         "required": true,
         "value": ""
      },
      "APP_ID": {
         "description": "Get API_ID from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      }
      
   }
}
